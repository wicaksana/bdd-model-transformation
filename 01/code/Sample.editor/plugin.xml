<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated sample -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="com.mde.sample.presentation.SampleModelWizardID"
            name="%_UI_SampleModelWizard_label"
            class="com.mde.sample.presentation.SampleModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SampleModelFile.gif">
         <description>%_UI_SampleModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated sample -->
      <editor
            id="com.mde.sample.presentation.SampleEditorID"
            name="%_UI_SampleEditor_label"
            icon="icons/full/obj16/SampleModelFile.gif"
            extensions="sample"
            class="com.mde.sample.presentation.SampleEditor"
            contributorClass="com.mde.sample.presentation.SampleActionBarContributor">
      </editor>
   </extension>

</plugin>
